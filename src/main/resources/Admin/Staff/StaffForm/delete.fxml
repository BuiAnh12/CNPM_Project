<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>

<DialogPane prefHeight="209.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.Controller.Staff.DeleteController">
   <header>
      <Pane prefHeight="200.0" prefWidth="398.0">
         <children>
            <Label layoutX="60.0" layoutY="38.0" text="Are you sure want to delete these users" />
            <Button fx:id="delete" layoutX="144.0" layoutY="141.0" mnemonicParsing="false" onAction="#deleteUserSelected" prefHeight="36.0" prefWidth="80.0" text="Delete">
               <styleClass>
                  <String fx:value="delete-button" />
                  <String fx:value="button" />
               </styleClass>
            </Button>
            <ComboBox fx:id="list" layoutX="61.0" layoutY="75.0" prefHeight="38.0" prefWidth="277.0" styleClass="input" />
         </children>
      </Pane>
   </header>
</DialogPane>
